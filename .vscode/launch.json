{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Main App",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/ingestion_app",
            "args": [
                "/media/knoller/Expansion/Data/courtlistener/bulk-2025-07-02/opinions-2025-07-02.csv",
                "--limit=100"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set unlimited string print length",
                    "text": "-gdb-set print elements 0",
                    "ignoreFailures": true
                },
                {
                    "description": "Disable debuginfod to avoid hangs",
                    "text": "-gdb-set debuginfod enabled off",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Project",
            "logging": {
                "engineLogging": false
            }
        },
